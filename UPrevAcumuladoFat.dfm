  object qPedido: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT SUM( DbpedidoItem.QtdParesRest ) QtdParesRest ,  Dbpedido' +
        'item.Preco, Dbpedidoitem.VlrDesconto'
      'FROM "dbPedido.DB" Dbpedido'
      '   INNER JOIN "dbPedidoItem.DB" Dbpedidoitem'
      '   ON  (Dbpedido.Pedido = Dbpedidoitem.Pedido)  '
      'WHERE   (Dbpedidoitem.QtdParesRest > 0)  '
      
        '   AND  ( (Dbpedido.Cancelado = FALSE)  OR  (Dbpedido.Cancelado ' +
        'IS NULL) )  '
      '   AND  (Dbpedido.Tipo = '#39'V'#39')  '
      'GROUP BY Dbpedidoitem.Preco, Dbpedidoitem.VlrDesconto')
    Left = 352
    Top = 312
    object qPedidoQtdParesRest: TFloatField
      FieldName = 'QtdParesRest'
      Origin = 'TABELAS."dbPedido.DB".QtdParesRest'
    end
