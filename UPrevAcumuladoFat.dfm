  object qAcumulado: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dbnotafiscal.NumNota, Dbnotafiscal.DtEmissao, Dbnotafisca' +
        'l.CodCli, Dbnotafiscalitens.CodProduto, SUM( Dbnotafiscalitens.Q' +
        'td ) Qtd, SUM( Dbnotafiscalitens.VlrTotal ) VlrTotal, Dbproduto.' +
        'Referencia, Dbproduto.Nome NomeProduto, Dbcliente.Nome, Dbclient' +
        'e.Fantasia, dbNotaFiscal.PercDesc'
      'FROM "dbNotaFiscal.DB" Dbnotafiscal'
      '   INNER JOIN "dbNotaFiscalItens.DB" Dbnotafiscalitens'
      '   ON  (Dbnotafiscal.NumNota = Dbnotafiscalitens.NumNota)'
      '   INNER JOIN "dbCliente.DB" Dbcliente'
      '   ON  (Dbnotafiscal.CodCli = Dbcliente.Codigo)'
      '   INNER JOIN "dbProduto.DB" Dbproduto'
      '   ON  (Dbnotafiscalitens.CodProduto = Dbproduto.Codigo)'
      'WHERE   (Dbnotafiscalitens.GeraDupl = TRUE)'
      
        '   AND  ( (Dbnotafiscal.Cancelada = FALSE)  OR  (Dbnotafiscal.Ca' +
        'ncelada IS NULL) )'
      
        '   AND  Dbnotafiscal.DtEmissao BETWEEN '#39'01/01/2006'#39' AND '#39'01/01/2' +
        '007'#39
      
        'GROUP BY Dbnotafiscal.NumNota, Dbnotafiscal.DtEmissao, Dbnotafis' +
        'cal.CodCli, Dbnotafiscalitens.CodProduto, Dbproduto.Referencia, ' +
        'Dbproduto.Nome, Dbproduto.Nome, Dbcliente.Nome, Dbcliente.Fantas' +
        'ia, dbNotaFiscal.PercDesc'
      'ORDER BY Dbcliente.Fantasia, Dbproduto.Referencia, NomeProduto'
      ' '
      ' ')
    Left = 444
    Top = 256
    object qAcumuladoNumNota: TIntegerField
      FieldName = 'NumNota'
    end
    object qAcumuladoDtEmissao: TDateField
      FieldName = 'DtEmissao'
    end
    object qAcumuladoCodCli: TIntegerField
      FieldName = 'CodCli'
    end
    object qAcumuladoCodProduto: TIntegerField
      FieldName = 'CodProduto'
    end
    object qAcumuladoQtd: TFloatField
      FieldName = 'Qtd'
    end
    object qAcumuladoVlrTotal: TFloatField
      FieldName = 'VlrTotal'
    end
    object qAcumuladoReferencia: TStringField
      FieldName = 'Referencia'
    end
    object qAcumuladoNomeProduto: TStringField
